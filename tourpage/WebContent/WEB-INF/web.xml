<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>tourpage</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>MemberFrontController</servlet-name>
    <servlet-class>tourpage.controller.MemberFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MemberFrontController</servlet-name>
    <url-pattern>*.me</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PlanFrontController</servlet-name>
    <servlet-class>tourpage.controller.PlanFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PlanFrontController</servlet-name>
    <url-pattern>*.pl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MyPageFrontController</servlet-name>
    <servlet-class>tourpage.controller.MyPageFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyPageFrontController</servlet-name>
    <url-pattern>*.mp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ShareFrontController</servlet-name>
    <servlet-class>tourpage.controller.ShareFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShareFrontController</servlet-name>
    <url-pattern>*.sp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ShopFrontController</servlet-name>
    <servlet-class>tourpage.controller.ShopFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShopFrontController</servlet-name>
    <url-pattern>*.sh</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GroupFrontController</servlet-name>
    <servlet-class>tourpage.controller.GroupFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GroupFrontController</servlet-name>
    <url-pattern>/GroupLogin.gw/*</url-pattern>
    <url-pattern>*.gw</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ChatSubmitController</servlet-name>
    <servlet-class>tourpage.controller.ChatSubmitController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChatSubmitController</servlet-name>
    <url-pattern>/ChatSubmitController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ChatListController</servlet-name>
    <servlet-class>tourpage.controller.ChatListController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChatListController</servlet-name>
    <url-pattern>/ChatListController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ChatUnreadController</servlet-name>
    <servlet-class>tourpage.controller.ChatUnreadController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChatUnreadController</servlet-name>
    <url-pattern>/ChatUnreadController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ChatBoxController</servlet-name>
    <servlet-class>tourpage.controller.ChatBoxController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChatBoxController</servlet-name>
    <url-pattern>/ChatBoxController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserRegisterCheckServlet</servlet-name>
    <servlet-class>tourpage.controller.UserRegisterCheckServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserRegisterCheckServlet</servlet-name>
    <url-pattern>/UserRegisterCheckServlet</url-pattern>
  </servlet-mapping>
  <resource-ref>
    <description>team</description>
    <res-ref-name>jdbc/jsptest</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
</web-app>